<!doctype html>
<html>
  <head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%= meta_data(current_page, 'title') %></title>
    <meta name="description" content="<%= meta_data current_page, 'description' %>" />
    <meta name="keywords" content="<%= meta_data current_page, 'keywords' %>" />
    <link rel="icon" href="https://d2yca1enuxtdrs.cloudfront.net/images/BSsAlCwH/jLnk3EmXr0fzIuBpLojH/rCOiMeDGO6.png" type="image/png">
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="https://d2yca1enuxtdrs.cloudfront.net/images/BSsAlCwH/jLnk3EmXr0fzIuBpLojH/rCOiMeDGO6.png">
    <script type="text/javascript" src="https://code.jquery.com/jquery-3.3.1.min.js"></script>
    <%= stylesheet_link_tag "application" %>
    <%= javascript_include_tag "application" %>
    <%= google_analytics_tag %>
    <script>
      (adsbygoogle = window.adsbygoogle || []).push({
        google_ad_client: "ca-pub-9118985146283136",
        enable_page_level_ads: true
      });
    </script>
  </head>
  <body>
    <%= yield %>
    <div class="footer">
      <div class="line">
        <a href="https://www.youtube.com/c/alexChaplinsky" class="icon-youtube" target="_blank"></a>
        <a href="https://www.instagram.com/alchaplinsky" class="icon-instagram" target="_blank"></a>
        <a href="https://www.facebook.com/alexchaplinsky" class="icon-facebook" target="_blank"></a>
      </div>
      <div class="muted">
        Made with <%= image_tag 'like.svg' %> by Alex Chaplinsky
      </div>
    </div>
  </body>
</html>
