<% I18n.locale = :ru %>
<!doctype html>
<html>
  <head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%= meta_data(current_page, 'title') %></title>
    <meta name="description" content="<%= meta_data current_page, 'description' %>" />
    <meta name="keywords" content="<%= meta_data current_page, 'keywords' %>" />
    <link rel="icon" href="https://d12swbtw719y4s.cloudfront.net/images/BSsAlCwH/fGCrn3FPSm2rJ3b5RDIO/6WEx2LdsKp.jpeg" type="image/png">
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="https://d12swbtw719y4s.cloudfront.net/images/BSsAlCwH/fGCrn3FPSm2rJ3b5RDIO/6WEx2LdsKp.jpeg">
    <script data-ad-client="ca-pub-9118985146283136" async src="https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
    <script type="text/javascript" src="https://code.jquery.com/jquery-3.3.1.min.js"></script>
    <%= stylesheet_link_tag "application" %>
    <%= javascript_include_tag "application" %>
    <%= google_analytics_tag %>
  </head>
  <body>
    <%= partial 'partials/header' %>
    <div class="blog">
      <div class="back">
        <a href="/blog"> &larr; Назад к списку</a>
      </div>
      <%= yield %>
      <div class="back">
        <a href="/blog"> &larr; Назад к списку</a>
      </div>
    </div>
    <%= partial 'partials/footer' %>
  </body>
</html>
